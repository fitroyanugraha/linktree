# Example Environment Variables
# Copy this file to .env and fill in your actual values

# SheetDB API Bearer Token
# Get this from your SheetDB dashboard
VITE_BEARER_TOKEN=your_api_token_here

# SheetDB API URL
# Format: https://sheetdb.io/api/v1/YOUR_SHEET_ID
VITE_API_URL=https://sheetdb.io/api/v1/your_sheet_id

# Maximum messages allowed per day per user
# Default: 2
VITE_MAX_MESSAGES_PER_DAY=2
